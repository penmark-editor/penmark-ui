{
  "name": "penmark",
  "version": "0.1.0",
  "description": "A React WYSIWYG editor component",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon --watch ./src -e ts,tsx,css --exec npm run build",
    "prepublishOnly": "npm run build",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "build:js": "babel ./src --out-dir ./dist --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shobokshy/penmark.git"
  },
  "keywords": [
    "React",
    "ProseMirror",
    "WYSIWYG",
    "Editor"
  ],
  "files": [
    "dist/*"
  ],
  "author": "shobokshy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shobokshy/penmark/issues"
  },
  "homepage": "https://github.com/shobokshy/penmark#readme",
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^12.7.2",
    "@types/prosemirror-commands": "^1.0.1",
    "@types/prosemirror-dropcursor": "^1.0.0",
    "@types/prosemirror-gapcursor": "^1.0.1",
    "@types/prosemirror-history": "^1.0.1",
    "@types/prosemirror-inputrules": "^1.0.2",
    "@types/prosemirror-keymap": "^1.0.1",
    "@types/prosemirror-model": "^1.7.2",
    "@types/prosemirror-schema-basic": "^1.0.1",
    "@types/prosemirror-schema-list": "^1.0.1",
    "@types/prosemirror-state": "^1.2.3",
    "@types/prosemirror-tables": "^0.8.0",
    "@types/prosemirror-transform": "^1.1.0",
    "@types/prosemirror-view": "^1.9.0",
    "@types/react": "^16.9.2",
    "babel-plugin-import": "^1.12.0",
    "prosemirror-history": "^1.0.4",
    "react": "^16.9.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@emotion/core": "^10.0.16",
    "antd": "^3.21.4",
    "prosemirror-commands": "^1.0.8",
    "prosemirror-dropcursor": "^1.1.1",
    "prosemirror-gapcursor": "^1.0.4",
    "prosemirror-inputrules": "^1.0.4",
    "prosemirror-keymap": "^1.0.1",
    "prosemirror-model": "^1.7.1",
    "prosemirror-schema-basic": "^1.0.1",
    "prosemirror-schema-list": "^1.0.3",
    "prosemirror-state": "^1.2.4",
    "prosemirror-tables": "^0.9.1",
    "prosemirror-transform": "^1.1.3",
    "prosemirror-utils": "^0.9.6",
    "prosemirror-view": "^1.10.0",
    "rpm-editor": "^2.0.0-alpha.2"
  }
}
